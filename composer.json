{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "axlon/laravel-postal-code-validation": "^3.3",
        "barryvdh/laravel-snappy": "^1.0",
        "coingate/coingate-php": "^4.1",
        "doctrine/dbal": "^3.4",
        "guzzlehttp/guzzle": "^7.2",
        "hisorange/browser-detect": "^4.5",
        "intervention/image": "^2.7",
        "jenssegers/agent": "^2.6",
        "josiasmontag/laravel-recaptchav3": "^1.0",
        "kyslik/column-sortable": "^6.4",
        "laravel/framework": "^9.19",
        "laravel/helpers": "^1.5",
        "laravel/sanctum": "^3.0",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.0",
        "livewire/livewire": "^2.10",
        "maatwebsite/excel": "^3.1",
        "mbarlow/megaphone": "^1.0",
        "mews/purifier": "^3.3",
        "nwidart/laravel-modules": "^9.0",
        "php-curl-class/php-curl-class": "^9.8",
        "propaganistas/laravel-phone": "^4.3",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "sonata-project/google-authenticator": "^2.3",
        "spatie/laravel-webhook-server": "^3.2",
        "stevebauman/location": "^6.5",
        "stichoza/google-translate-php": "^4.1",
        "stripe/stripe-php": "^9.7",
        "twilio/sdk": "^6.42"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "kkomelin/laravel-translatable-string-exporter": "^1.17",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Services\\": "Services/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
